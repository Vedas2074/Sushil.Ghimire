@model List<Employee>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">#</th>
                <th scope="col">FirstName</th>
                <th scope="col">LastName</th>
                <th scope="col">Adderess</th>
                <th scope="col">Age</th>
                <th scope="col">Salary</th>


            </tr>
        </thead>

@foreach (var item in Model)
{
        <tbody>
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                
                <td>@item.Address</td>
                <td>@item.Age</td>
                <td>@item.Salary</td>


            </tr>
            
        </tbody>
    
}
</table>
