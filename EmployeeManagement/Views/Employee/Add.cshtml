@model Employee

<h1 class="text-center">Form FillUp</h1>

<form>
    <div class="mb-3">
        <label for="firstname" class="form-label">First Name:</label>
        <input type="text" class="form-control" id="firstName" aria-describedby="" placeholder="Enter F">
    </div>
    <div class="mb-3">
        <label for="lastname" class="form-label">Last Name:</label>
        <input type="text" class="form-control" id="lastName" aria-describedby="">
    </div>
    <div class="mb-3">
        <label for="firstName" class="form-label">Address:</label>
        <input type="text" class="form-control" id="firstName" aria-describedby="">
    </div>
    <div class="mb-3">
        <label for="age" class="form-label">Current Age:</label>
        <input type="text" class="form-control" id="age" aria-describedby="">
    </div>
    <div class="mb-3">
        <label for="salary" class="form-label">Salary:</label>
        <input type="text" class="form-control" id="salary" aria-describedby="">
    </div>
